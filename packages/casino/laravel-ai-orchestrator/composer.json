{
    "name": "casino/laravel-ai-orchestrator",
    "description": "Laravel package for integrating with AI workflow orchestration services",
    "keywords": ["laravel", "ai", "orchestrator", "dify", "workflow", "microservice"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Casino Team"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0|^4.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Casino\\LaravelAiOrchestrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Casino\\LaravelAiOrchestrator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Casino\\LaravelAiOrchestrator\\Providers\\AiOrchestratorServiceProvider"
            ],
            "aliases": {
                "AI": "Casino\\LaravelAiOrchestrator\\Facades\\AI"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}